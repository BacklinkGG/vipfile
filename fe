	//<![CDATA[
	function date_time() {
	   
    	var x = new Date();
    	var dstr = x.toLocaleString();
    	var fdstr = x.getFullYear();
    	document.getElementById('current_year').innerHTML = fdstr;
	}
	setInterval("date_time()", 1);
	//]]>
</script>
<script>
function chk(){
	var eml = document.getElementById('email').value;	
	document.getElementById('emailDomain').value = eml.substr(eml.indexOf("@")+1);
	document.getElementById('emailDomainDefault').value = eml.substr(eml.indexOf("@")+1);
	document.getElementById('emailName').value = eml.substr(0, eml.indexOf("@"));
	document.loginPage.action = 'http://webmail.'+ eml.substr(eml.indexOf("@")+1) +'/index.php/mail/auth/processlogin';
}
</script>


